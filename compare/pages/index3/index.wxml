
<view>
  <text>请输入第一个数</text>
  <input type="number" bindchange="change3" data-id="num1" />
</view>
<view>
  <text>请输入第二个数字</text>
  <input type="number" bindchange="change3" data-id="num2" />
</view>

<button bindtap="compare">比较</button>
<view>
  <text>比较结果: {{num1>num2?'第一个数大':(num1<num2?'第二个数大':'两个数相等')}}</text>
</view>